@page
@model RegisterModel

<h2>Register</h2>

<form method="post">
    <div>
        <label>Email</label>
        <input asp-for="Input.Email" />
    </div>
    <div>
        <label>Password</label>
        <input asp-for="Input.Password" type="password" />
    </div>
    <button type="submit">Register</button>
</form>
