@page
@model LoginModel

<h2>Login</h2>

<form method="post">
    <div>
        <label>UserName</label>
        <input asp-for="Input.UserName" />
    </div>
    <div>
        <label>Password</label>
        <input asp-for="Input.Password" type="password" />
    </div>
    <button type="submit">Login</button>
</form>
